{
    "enabled": "true",
    "increase-coverage": {
      "project-name": "eval-java-spring-boot-boilerplate",
      "target-branches": [
        "main"
      ],
      "test-env-docker-stage": "test-dependencies",
      "build-test-env-docker-stage-script-path": "tool/build-test-env-docker-stage.sh",
      "test-files-folders": [
        "src",
        "coverage"
      ],
      "source-test-file-paths": [
        {
          "source-file-path": "src/main/java/com/mewebstudio/javaspringbootboilerplate/controller/AuthController.java",
          "test-file-path": "src/test/java/com/mewebstudio/javaspringbootboilerplate/controller/AuthControllerTest.java"
        }
      ],
      "test-command": "mvn clean test verify -Dmaven.test.failure.ignore=true",
      "coverage-type": "jacoco",
      "code-coverage-report-path": "coverage/jacoco.csv",
      "desired-coverage": 90,
      "max-iterations": 1,
      "max-number-of-test-cases": 6
    }
  }